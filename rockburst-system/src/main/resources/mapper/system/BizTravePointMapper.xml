<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BizTravePointMapper">
    <!--获取同一巷道，最大的导线点编号-->
    <select id="selectMaxNo" resultType="java.lang.Integer">
        select ifnull(max(no), 0)
        from biz_trave_point as btp
        where btp.tunnel_id = #{tunnelId} and btp.del_flag = '0'
    </select>
</mapper>